<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:AnimaFiltering.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:AnimaFiltering.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="600"
		MinHeight="600"
	    MinWidth="800"
		MaxHeight="600"
		MaxWidth="800"
        x:Class="AnimaFiltering.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="AnimaFiltering"
		CanResize="False">
	<TabControl SelectionChanged="TabControl_SelectionChanged">
		<TabControl.Items>
			<TabItem Header="Main Menu">
				<views:MainView Margin="0 40 0 0"/>
			</TabItem>
			<TabItem Header="Stats">
				<views:StatsView Margin="0 40 0 0"/>
			</TabItem>
		</TabControl.Items>

	</TabControl>
</Window>
